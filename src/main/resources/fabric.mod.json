{
	"schemaVersion": 1,
	"id": "mymod",
	"version": "${version}",
	"name": "mymod",
	"description": "this is a mod for learning minecraft modding and having fun with it",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mymod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.mymod.Mymod"
		],
		"client": [
			"com.mymod.MyModClient"
		],
		"fabric-datagen": [
			"com.mymod.MymodDataGenerator"
		]


	},
	"mixins": [
		"mymod.mixins.json",
		{
			"config": "mymod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}